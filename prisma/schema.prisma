generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id        Int      @id @default(autoincrement())
  firstname String   @db.VarChar(50)
  lastname  String   @db.VarChar(50)
  createdAt DateTime @default(now())
}
